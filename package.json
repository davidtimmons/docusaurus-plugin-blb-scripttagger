{
  "name": "docusaurus-plugin-blb-scripttagger",
  "version": "1.0.4",
  "description": "BLB (Blue Letter Bible) ScriptTagger plugin for Docusaurus.",
  "keywords": [
    "docusaurus",
    "plugin",
    "blb scripttagger",
    "blue letter bible",
    "bible",
    "scripture",
    "references"
  ],
  "homepage": "https://github.com/davidtimmons/docusaurus-plugin-blb-scripttagger",
  "bugs": {
    "url": "https://github.com/davidtimmons/docusaurus-plugin-blb-scripttagger/issues"
  },
  "license": "MIT",
  "author": "David Timmons <npm@timmons.io> (https://github.com/davidtimmons)",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtimmons/docusaurus-plugin-blb-scripttagger.git"
  },
  "scripts": {
    "build": "yarn build:browser && yarn build:server",
    "build:browser": "tsc --project ./tsconfig.browser.json",
    "build:server": "tsc --project ./tsconfig.server.json",
    "clean": "rm -rf ./lib",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,md,yml,json}",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@docusaurus/utils-validation": "^2.0.0-beta.15"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.15",
    "@docusaurus/types": "^2.0.0-beta.15",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@docusaurus/core": "2.0.0-beta.12 - 2.x"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
